<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.myrun4">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name="com.example.myrun4.activity.MapActivity"
            android:label="Map"
            android:parentActivityName="com.example.myrun4.activity.MainActivity">
        </activity>
        <!--<activity-->
            <!--android:name=".ProfileActivity"-->
            <!--android:label="Profile"-->
            <!--android:parentActivityName=".MainActivity" />-->
        <activity
            android:name="com.example.myrun4.activity.Settings"
            android:label="Settings"
            android:parentActivityName="com.example.myrun4.activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.myrun4.activity.MainActivity" />
        </activity>
        <activity
            android:name="com.example.myrun4.activity.MainActivity"
            android:label="@string/MainActivity"
            android:parentActivityName="com.example.myrun4.activity.LoginActivity">
        </activity>
        <activity android:name="com.example.myrun4.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.myrun4.activity.RegisterActivity"
            android:label="@string/Register_head"
            android:parentActivityName="com.example.myrun4.activity.LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.myrun4.activity.LoginActivity" />
        </activity>
        <activity android:name="com.soundcloud.android.crop.CropImageActivity" />
        <activity
            android:name="com.example.myrun4.activity.Manal_Entry"
            android:label="Manual Entry Activity"
            android:parentActivityName="com.example.myrun4.activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.myrun4.activity.MainActivity" />
        </activity>
    </application>

</manifest>